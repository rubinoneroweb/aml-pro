<%= simple_form_for(@job) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :operator_id %>
    <%= f.input :started_at %>
    <%= f.input :completed_at %>
    <%= f.input :metalwork_id %>
    <%= f.input :order_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
